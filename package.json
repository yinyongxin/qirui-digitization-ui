{
  "name": "qirui-digitization-ui",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "private": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/qirui-digitization-ui.umd.cjs",
  "module": "./dist/qirui-digitization-ui.js",
  "exports": {
    ".": {
      "import": "./dist/qirui-digitization-ui.js",
      "require": "./dist/qirui-digitization-ui.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./icons/css": {
      "import": "./dist/icons/css.less",
      "require": "./dist/icons/css.less"
    },
    "./icons/less": {
      "import": "./dist/icons/less.less",
      "require": "./dist/icons/less.less"
    }
  },
  "version": "1.1.4",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
