{
  "name": "qirui-digitization-ui",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "private": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/qirui-digitization-ui.umd.cjs",
  "module": "./dist/qirui-digitization-ui.js",
  "exports": {
    ".": {
      "import": "./dist/qirui-digitization-ui.js",
      "require": "./dist/qirui-digitization-ui.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./icons/css": {
      "import": "./dist/icons/css.css",
      "require": "./dist/icons/css.css"
    },
    "./icons/less": {
      "import": "./dist/icons/less.less",
      "require": "./dist/icons/less.less"
    },
    "./theme": {
      "import": "./dist/theme/index.css",
      "require": "./dist/theme/index.css"
    },
    "./theme/light": {
      "import": "./dist/theme/light/index.css",
      "require": "./dist/theme/light/index.css"
    },
    "./theme/dark": {
      "import": "./dist/theme/dark/index.css",
      "require": "./dist/theme/dark/index.css"
    },
    "./globalLess": {
      "import": "./dist/globalLess/index.less",
      "require": "./dist/globalLess/index.less"
    }
  },
  "version": "1.5.2",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --host",    
    "build:comp": "vite build --mode comp && tsc -p tsconfig.build.json",
    "build:docu": "vite build --mode docu",
    "preview": "vite preview"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@types/loadable__component": "^5.13.4",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
